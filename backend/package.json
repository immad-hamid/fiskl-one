  "description": "Invoice Management Backend with PostgreSQL and Prisma",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "proxy": "node proxy-server.js",
    "start:with-proxy": "concurrently \"npm run proxy\" \"npm start\"",
    "dev:with-proxy": "concurrently \"npm run proxy\" \"npm run dev\"",
